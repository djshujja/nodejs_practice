<div>
  <h2>Welcome to Library</h2>
</div>
<div>
  <h4 class="page-header">Recently Added</h4>
</div>
<div>
  <% books.forEach(book => {%>
  <a href="/books/<%= book.id %>">
    <img
      src="<%= book.coverImagePath %> "
      alt="coverImage"
      height="150"
      width="100"
  /></a>
  <% }) %>
</div>
